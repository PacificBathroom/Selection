{
  "compilerOptions": {
    "target": "ES2020",
    "lib": [
      "ES2020",
      "DOM"
    ],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "noEmit": true,
    "types": [
      "@netlify/functions"
    ]
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
    "netlify/functions"
  ]
}
